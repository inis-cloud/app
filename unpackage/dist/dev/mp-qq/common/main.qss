.alert::before {
    background: #38a3fd;
    border-radius: 50%;
    color: #fff;
    content: "i";
    font-family: Dosis,"Source Sans Pro","Helvetica Neue",Arial,sans-serif;
    font-size: 16px;
    height: 21px;
    line-height: 21px;
    margin-left: -32px;
    position: absolute;
    text-align: center;
    width: 21px;
}
.alert.bg-secondary::before, .alert.alert-secondary::before, .alert.alert-dark::before{background:#313a46;}
.alert.bg-secondary, .alert.alert-secondary, .alert.alert-dark{border-left: 3px solid #313a46;}
.alert.bg-white::before, .alert.bg-info::before{background:#e3eaef;}
.alert.bg-white, .alert.bg-info{border-left: 3px solid #e3eaef;}
.alert.bg-danger::before{background:red;content: "×";}
.alert.bg-danger{border-left: 3px solid red;}
.alert.alert-danger::before{background:#fa5c7c;content: "×";}
.alert.alert-danger{border-left: 3px solid #fa5c7c;}
.alert.bg-warning::before, .alert.alert-warning::before{background:#ff7800;content: "!";}
.alert.bg-warning, .alert.alert-warning{border-left: 3px solid #ff7800;}
.alert.bg-info::before, .alert.bg-success::before{background:#727cf5;}
.alert.bg-info, .alert.bg-success{border-left: 3px solid #727cf5;}
.alert.bg-success::before{content: "√";}
.alert.alert-success::before, .alert.bg-primary::before{background:#727cf5;content: "√";}
.alert.alert-success, .alert.bg-primary{border-left: 3px solid #727cf5;}
.alert.alert-primary::before{background:#727cf5;content: "√";}
